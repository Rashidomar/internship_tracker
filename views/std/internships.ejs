</head>

<body>
    <div class="container">
        <header class="d-flex flex-wrap justify-content-center py-3 mb-4 border-bottom">
            <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-dark text-decoration-none">
                <svg class="bi me-2" width="40" height="32">
                    <use xlink:href="#bootstrap" /></svg>
                <span class="fs-4">Internship Tracker</span>
            </a>
            <ul class="nav nav-pills">
                <li class="nav-item"><a href="#" class="nav-link">Generate Summary</a>

                    <li class="nav-item"><a href="/logout" class="nav-link">Sign Out</a>
                    </li>
            </ul>
        </header>
    </div>
    <div class="container-fliud">
        <div class="row">
            <div class="col-md-3  bg-light rounded-3" style="height:fit-content;">
                <a href="/" class="d-flex align-items-center pb-3 mb-3 link-dark text-decoration-none border-bottom">
                    <svg class="bi pe-none me-2" width="30" height="24">
                        <use xlink:href="#bootstrap" /></svg>
                    <span class="fs-5 fw-semibold"><%= user.firstname %></span>
                </a>
                <ul class="list-unstyled ps-0">
                    <li class="mb-1">
                        <button class="btn btn-toggle d-inline-flex align-items-center rounded border-0 collapse" data-bs-toggle="collapse" data-bs-target="#home-collapse" aria-expanded="false">
                            Internship Experience
                        </button>
                        <div class="collapse show" id="home-collapse">
                            <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                                <li><a href="#" class="link-dark d-inline-flex text-decoration-none rounded" data-bs-toggle="modal" data-bs-target="#internship">Add New</a>
                                    <!-- Modal Internship-->
                                    <div class="modal fade" id="internship" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="staticBackdropLabel">New Internship</h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                </div>
                                                <div class="modal-body">
                                                    <form action="/add_internship" method="Post" class="row g-3">
                                                        <div class="col-md-6">
                                                            <label for="inputEmail4" class="form-label">Company</label>
                                                            <input type="text" name="company" class="form-control" id="inputEmail4">
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label for="inputPassword4" class="form-label">Role</label>
                                                            <input type="text" name="role" class="form-control" id="inputPassword4">
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="inputAddress" class="form-label">Your
                                                                Task</label>
                                                            <input type="text" name="task" class="form-control" id="inputAddress" placeholder="1234 Main St">
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="inputAddress" class="form-label">Duration</label>
                                                            <input type="text" name="duration" class="form-control" id="inputAddress" placeholder="1234 Main St">
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="inputCity" class="form-label">Your
                                                                Experience</label>
                                                            <textarea class="form-control" name="experience" placeholder=" Leave a comment here " id="floatingTextarea2 " style="height: 100px "></textarea>
                                                        </div>
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" class="btn btn-secondary " data-bs-dismiss="modal ">Close</button>
                                                    <button type="submit " class="btn btn-primary ">Submit</button>
                                                </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li>
                                    <!-- <a href="#" class="link-dark d-inline-flex text-decoration-none rounded" data-bs-toggle="modal" data-bs-target="#edit_internship">Edit</a> -->
                                </li>
                                <li><a href="#" class="link-dark d-inline-flex text-decoration-none rounded">View</a>
                                </li>
                            </ul>
                        </div>
                    </li>
                    <li class="mb-1">
                        <button class="btn btn-toggle d-inline-flex align-items-center rounded border-0 collapsed" data-bs-toggle="collapse" data-bs-target="#dashboard-collapse" aria-expanded="false">
                            Certs And Documents
                        </button>
                        <div class="collapse" id="dashboard-collapse">
                            <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                                <li><a href="#" class="link-dark d-inline-flex text-decoration-none rounded" data-bs-toggle="modal" data-bs-target="#document">Add</a>
                                    <!-- Modal Certts-->
                                    <div class="modal fade" id="document" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                        <div class="modal-dialog ">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="staticBackdropLabel">New Certifications/Documents
                                                    </h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                </div>
                                                <div class="modal-body">
                                                    <form action="/add_document" method="Post" enctype="multipart/form-data" class="row g-3">
                                                        <div class="col-md-6">
                                                            <label for="inputEmail4" class="form-label">Name</label>
                                                            <input type="text" name="d_name" class="form-control" id="inputEmail4">
                                                        </div>
                                                        <div class="mb-3 col-12">
                                                            <label for="formFile" class="form-label">Select file</label>
                                                            <input class="form-control" type="file" name="cert" id="formFile">
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label for="inputCity" class="form-label">Your Experience</label>
                                                            <textarea class="form-control " name="experience" placeholder=" Leave a Experience here " id="floatingTextarea2" style="height: 100px "></textarea>
                                                        </div>

                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                                    <button type="submit" class="btn btn-primary">Submit</button>
                                                </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li>
                                    <!-- <a href="#" class="link-dark d-inline-flex text-decoration-none rounded" data-bs-toggle="modal" data-bs-target="#edit_document">Edit</a> -->
    
                                    <a href="#" class="link-dark d-inline-flex text-decoration-none rounded">View</a>
                                </li>
                            </ul>
                        </div>
                    </li>
                    <li class="mb-1 ">
                        <button class="btn btn-toggle d-inline-flex align-items-center rounded border-0 collapsed" data-bs-toggle="collapse" data-bs-target="#orders-collapse" aria-expanded="false">
                            Projects
                        </button>
                        <div class="collapse" id="orders-collapse">
                            <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small">
                                <li><a href="#" class="link-dark d-inline-flex text-decoration-none rounded" data-bs-toggle="modal" data-bs-target="#project">Add New Project</a>
                                     <!-- Modal Project-->
                                    <div class="modal fade" id="project" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="staticBackdropLabel">New Project</h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                </div>
                                                <div class="modal-body">
                                                    <form action="/add_project" method="Post" class="row g-3">
                                                        <div class="col-md-6">
                                                            <label for="inputEmail4 " class="form-label ">Name</label>
                                                            <input type="text" name="p_name" class="form-control" id="inputEmail4 ">
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label for="inputPassword4" class="form-label">Duration</label>
                                                            <input type="text" name="duration" class="form-control" id="inputPassword4">
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="inputAddress" class="form-label ">Project
                                                                url</label>
                                                            <input type="text" name="url" class="form-control" id="inputAddress" placeholder="1234 Main St ">
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="inputCity" class="form-label">Description</label>
                                                            <textarea class="form-control" name="description" placeholder=" Leave a description here " id="floatingTextarea2 " style="height: 100px "></textarea>
                                                        </div>
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                                    <button type="submit" class="btn btn-primary">Submit</button>
                                                </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li><a href="#" class="link-dark d-inline-flex text-decoration-none rounded">View Projects</a></li>
                                <!-- <li><a href="#" class="link-dark d-inline-flex text-decoration-none rounded" data-bs-toggle="modal" data-bs-target="#edit_project">Edits</a></li> -->
                           
                            </ul>
                        </div>
                    </li>
                    <li class="border-top my-3 "></li>
                    <li class="mb-1 ">
                        <button class="btn btn-toggle d-inline-flex align-items-center rounded border-0 collapsed" data-bs-toggle="collapse" data-bs-target="#account-collapse " aria-expanded="true ">
                            Account
                        </button>
                        <div>
                            <ul class="btn-toggle-nav list-unstyled fw-normal pb-1 small ">
                                <li><a href="/profile " class="link-dark d-inline-flex text-decoration-none rounded ">Profile</a></li>
                                <li><a href="# " class="link-dark d-inline-flex text-decoration-none rounded ">Settings</a></li>
                            </ul>
                        </div>
                    </li>
                </ul>
            </div>
            <div class="col-md-9" style="height: auto;">
                <div class="p-5 mb-4 bg-light rounded-3">
                    <div class="container-fluid py-5">
                        <h1 class="display-5 fw-bold">Your Internship</h1>
                        <div class="card mb-4">
                            <div class="card-body">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th scope="col">Company</th>
                                            <th scope="col">role</th>
                                            <th scope="col">task</th>
                                            <th scope="col">experience</th>
                                            <th scope="col">duration</th>
                                            <th scope="col">Edit</th>
                                            <th scope="col">Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody class="table-hover">
                                        <% interns.forEach(function(intern){ %>
                                            <% if (intern.user_id  == user.id) { %>
                                                <tr>
                                                    <td><%=intern.company%></td>
                                                    <td><%=intern.role%></td>
                                                    <td><%=intern.task%></td>
                                                    <td><%=intern.experience%></td>
                                                    <td> <%=intern.duration%></td>
                                                    <td><button type="button" class="btn btn-warning edit_internship" 
                                                        data-bs-toggle="modal" 
                                                        data-bs-target="#edit_internship" 
                                                        data-internship_id="<%=intern.id%>" 
                                                        data-company="<%=intern.company%>" 
                                                        data-role="<%=intern.role%>" 
                                                        data-task=" <%=intern.task%>" 
                                                        data-experience="<%=intern.experience%>" 
                                                        data-duration="<%=intern.duration%>">Edit</button>
                                                    </td>
                                                    <td><button type="button" class="btn btn-danger internship_delete" data-id="<%=intern.id%>">Delete</button></td>
                                                </tr>
                                            <% } %>
                                        <%})%>
                                    </tbody>
                                </table>
                                <!-- Modal Internship Edit-->
                                <div class="modal fade" id="edit_internship" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="staticBackdropLabel">Edit Internship</h5>
                                                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                            </div>
                                            <div class="modal-body">
                                                <form action="/edit_internship" method="Post" class="row g-3">
                                                    <input type="hidden" name="internship_id" id="internship_id">
                                                    <div class="col-md-6">
                                                        <label for="inputEmail4" class="form-label">Company</label>
                                                        <input type="text" name="company" class="form-control" id="company">
                                                    </div>
                                                    <div class="col-md-6">
                                                        <label for="inputPassword4" class="form-label">Role</label>
                                                        <input type="text" name="role" class="form-control" id="role">
                                                    </div>
                                                    <div class="col-12">
                                                        <label for="inputAddress" class="form-label">Your Task</label>
                                                        <input type="text" name="task" class="form-control" id="task" placeholder=" 1234 Main St ">
                                                    </div>
                                                    <div class="col-12 ">
                                                        <label for="inputAddress" class="form-label">Duration</label>
                                                        <input type="text" name="duration" class="form-control" id="duration" placeholder="1234 Main St">
                                                    </div>
                                                    <div class="col-12">
                                                        <label for="inputCity" class="form-label">Your Experience</label>
                                                        <textarea class="form-control " name="experience" placeholder="Leave a comment here" id="experience" style="height: 100px"></textarea>
                                                    </div>
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                                <button type="submit" class="btn btn-primary">Submit</button>
                                            </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row align-items-md-stretch ">
                    <div class="col-md-6 h-100 p-5 bg-light border rounded-3 ">
                        <h2>Your Projects</h2>
                        <div class="card mb-4 ">
                            <div class="card-body ">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th scope="col">Project Name</th>
                                            <th scope="col">Duration</th>
                                            <th scope="col">URL</th>
                                            <th scope="col">Description</th>
                                            <th scope="col">Edit</th>
                                            <th scope="col">Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody >
                                        <% projects.forEach(function(project){ %>
                                            <% if (project.user_id  == user.id) { %>
                                                <tr>
                                                    <td><%=project.p_name%></td>
                                                    <td><%=project.duration%></td>
                                                    <td><%=project.url%></td>
                                                    <td><%=project.description%></td>
                                                    <td><button type="button" 
                                                        data-bs-toggle="modal" 
                                                        data-bs-target="#edit_project"
                                                        data-project_id="<%=project.id%>" 
                                                        data-p_name="<%=project.p_name%>" 
                                                        data-p_duration="<%=project.duration%>" 
                                                        data-url="<%=project.url%>" 
                                                        data-description="<%=project.description%>"  
                                                        class="btn btn-info edit_project">Edit</button></td>
                                                    <td><button type="button" class="btn btn-danger project_delete" data-id="<%=project.id%>">Delete</button></td>
                                                </tr>
                                            <% } %>
                                        <%})%>
                                    </tbody>
                                </table>
                                     <!-- Modal Edit Project-->
                                     <div class="modal fade" id="edit_project" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="staticBackdropLabel">Edit Project</h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                </div>
                                                <div class="modal-body">
                                                    <form action="/edit_project" method="Post" class="row g-3">
                                                        <input type="hidden" name="project_id" id="project_id">
                                                        <div class="col-md-6">
                                                            <label for="inputEmail4" class="form-label">Name</label>
                                                            <input type="text" name="p_name" class="form-control" id="p_name">
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label for="inputPassword4" class="form-label">Duration</label>
                                                            <input type="text" name="duration" class="form-control" id="p_duration">
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="inputAddress" class="form-label">Project url</label>
                                                            <input type="text" name="url" class="form-control" id="url">
                                                        </div>
                                                        <div class="col-12">
                                                            <label for="inputCity" class="form-label">Description</label>
                                                            <textarea class="form-control" name="description" id="description" style="height: 100px"></textarea>
                                                        </div>
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                                    <button type="submit" class="btn btn-primary">Submit</button>
                                                </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="h-100 p-5 bg-light border rounded-3">
                            <h2>Your Documents and Certs</h2>
                            <div class="card mb-4 ">
                                <div class="card-body">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr>
                                                <th scope="col">#</th>
                                                <th scope="col">Document Name</th>
                                                <th scope="col">Experience</th>
                                                <th scope="col">Edit</th>
                                                <th scope="col">Delete</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <% documents.forEach(function(document){ %>
                                                <% if (document.user_id  == user.id) { %>
                                                    <tr>
                                                        <th scope="row">1</th>
                                                        <td> <%=document.d_name%> </td>
                                                        <td> <%=document.experience%></td>
                                                        <td><button type="button" 
                                                            data-bs-toggle="modal" 
                                                            data-bs-target="#edit_document"
                                                            data-document_id="<%=document.id%>" 
                                                            data-d_name="<%=document.d_name%>" 
                                                            data-d_experience="<%=document.experience%>" 
                                                            class="btn btn-info edit_document">Edit</button></td>
                                                        <td><button type="button" class="btn btn-danger document_delete" data-id="<%=document.id%>">Delete</button></td>
                                                    </tr>
                                                <% } %>
                                            <%})%>

                                        </tbody>
                                    </table>
                                        <!-- Modal Edit Cert-->
                                        <div class="modal fade" id="edit_document" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                                            <div class="modal-dialog">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <h5 class="modal-title" id="staticBackdropLabel">Edit Certifications/Documents</h5>
                                                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close "></button>
                                                    </div>
                                                    <div class="modal-body ">
                                                        <form action="/edit_document" method="Post" class="row g-3">
                                                            <input type="hidden" name="document_id" id="document_id">
                                                            <div class="col-md-6">
                                                                <label for="inputEmail4" class="form-label">Name</label>
                                                                <input type="text " name="d_name" class="form-control" id="d_name">
                                                            </div>
                                                            <div class="col-md-6 ">
                                                                <label for="inputCity" class="form-label ">Your Experience</label>
                                                                <textarea class="form-control" name="experience" id="d_experience" style="height: 100px"></textarea>
                                                            </div>
                                                    </div>
                                                    <div class="modal-footer">
                                                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                                                        <button type="submit" class="btn btn-primary">Submit</button>
                                                    </div>
                                                    </form>
                                                </div>
                                            </div>
                                        </div>
                                </div>
                            </div>
                        </div>
